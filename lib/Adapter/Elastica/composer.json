{
    "name": "pagerfanta/elastica-adapter",
    "type": "library",
    "description": "Pagerfanta adapter for Elastica",
    "keywords": ["pagerfanta", "elastica"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "pagerfanta/core": "^3.7 || ^4.0",
        "ruflin/elastica": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6 || ^10.0",
        "symfony/phpunit-bridge": "^5.4 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Pagerfanta\\Elastica\\": "./"
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "minimum-stability": "dev"
}
