{
    "name": "pagerfanta/doctrine-collections-adapter",
    "type": "library",
    "description": "Pagerfanta adapter for Doctrine Collections",
    "keywords": ["pagerfanta", "doctrine", "collections"],
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "doctrine/collections": "^1.8 || ^2.0",
        "pagerfanta/core": "^3.0",
        "symfony/deprecation-contracts": "^2.1 || ^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "Pagerfanta\\Doctrine\\Collections\\": "./"
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "minimum-stability": "dev"
}
