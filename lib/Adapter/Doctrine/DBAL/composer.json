{
    "name": "pagerfanta/doctrine-dbal-adapter",
    "type": "library",
    "description": "Pagerfanta adapter for Doctrine DBAL",
    "keywords": ["pagerfanta", "doctrine", "dbal"],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "doctrine/dbal": "^3.5",
        "pagerfanta/core": "^3.7 || ^4.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.2"
    },
    "autoload": {
        "psr-4": {
            "Pagerfanta\\Doctrine\\DBAL\\": "./"
        },
        "exclude-from-classmap": [
            "Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Pagerfanta\\Doctrine\\DBAL\\Tests\\": "Tests"
        }
    },
    "minimum-stability": "dev"
}
